<h1>Articles!</h1>


<ul>
      <% @articles.each do |article| %>

    <li>
        <%= "Title: " + article[0] + "." %>
        <%= "Description:" + article[1]+ "." %>
        <%= "URL:" + article[2]+ "." %>
    </li>

      <% end %>
    </ul>

    <h4 class = 'new'><a href="/articles/new">Add A New Article</a></h4>

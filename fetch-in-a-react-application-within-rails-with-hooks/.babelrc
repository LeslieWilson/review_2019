{
  "presets": [
    [
      "@babel/env",
      {
        "corejs": 2,
        "forceAllTransforms": true,
        "modules": false,
        "targets": {
          "browsers": "> 1%"
        },
        "useBuiltIns": "usage"
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    [
      "@babel/proposal-class-properties",
      {
        "spec": true
      }
    ],
    "@babel/proposal-object-rest-spread",
    "@babel/syntax-dynamic-import"
  ]
}
